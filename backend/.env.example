# Django Settings
DJANGO_SETTINGS_MODULE=listAPI.production_settings
SECRET_KEY=your-secret-key-here-generate-a-new-one-for-production
DEBUG=False

# Database (Railway will provide this automatically)
DATABASE_URL=postgresql://user:password@host:port/database

# Frontend URL (your Vercel domain)
FRONTEND_URL=https://your-app.vercel.app
ALLOWED_HOSTS=your-railway-app.railway.app,your-custom-domain.com

# Railway will set these automatically:
# RAILWAY_PUBLIC_DOMAIN
# DATABASE_URL (when you add a Postgres service)
